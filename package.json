{
    "name": "openvegemap-cordova",
    "license": "GPL-3.0",
    "description": "OpenVegeMap Cordova app",
    "repository": "https://github.com/Rudloff/openvegemap-cordova.git",
    "dependencies": {
        "openvegemap": "0.2.0",
        "cordova-android": "~6.1.2",
        "cordova-plugin-exclude-files": "~0.2.0",
        "cordova-plugin-geolocation": "~2.4.3",
        "cordova-plugin-overview": "~0.1.0",
        "cordova-plugin-whitelist": "1",
        "cordova-plugin-statusbar": "~2.2.0"
    },
    "scripts": {
        "postinstall": "cd www/; npm install; bower install; grunt",
        "clean": "rm -r platforms/ plugins/; cordova prepare"
    },
    "cordova": {
        "platforms": [
            "android"
        ],
        "plugins": {
            "cordova-plugin-exclude-files": {},
            "cordova-plugin-geolocation": {},
            "cordova-plugin-overview": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {}
        }
    }
}